" File: ~/.vim/vimpagerrc vimpager(1) vim:ft=vim:

" Note: vimpager gets stuck if sourcing this file errors
" Also: export MANPAGER='vim -u ~/.vim/vimpagerrc -M +MANPAGER'

"set noloadplugins
"set verbose=9 verbosefile=/tmp/vimdebug.log

if has('vim_starting')
  call init#source_rc('init')
endif
silent! call init#source_rc('options')
silent! call init#source_rc('local')
silent! call init#do_plugins()
silent! call init#do_syntax()

set hls nonumber norelativenumber signcolumn=no foldcolumn=0

let g:vimpager = {'X11':0}
let g:less     = {'enabled':1,'number':0}
